package TikiShopMVC.Service.User;

import org.springframework.stereotype.Service;

import TikiShopMVC.Dto.PaginateDto;

@Service
public class PaginateImplService {
	public PaginateDto GetInforPaginate(int totalPage, int limit, int currentPage) {
		PaginateDto paginate = new PaginateDto();
		paginate.setCurrentPage(currentPage);
		paginate.setLimit(limit);
		paginate.setTotalPage(totalPage);
		
		return paginate;
	}
}
